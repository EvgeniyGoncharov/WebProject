@model IEnumerable<WebProject.Models.Employee>

<!DOCTYPE html>
<html>

	<head>

		<title>Сотрудники</title>
	</head>
	<body>
		<a href="/home/Index">На главную</a>
		<table>
			<thead>
				<tr>	
					<th>ID</th>
					<th>Фамилия</th>
					<th>Имя</th>
					<th>Возраст</th>
				</tr>
			</thead>
			<tbody>
			@foreach (var employee in Model)
			{
				<tr>
					<td>@employee.Id</td>
					<td>@employee.FirstName</td>
					<td>@employee.LastName</td>
					<td>@employee.Age</td>
				</tr>
			}
			</tbody>

		</table>
	</body>
</html>
